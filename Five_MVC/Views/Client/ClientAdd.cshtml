<script>
    function zhu() {
        var a = $("#Text5").val();
        var b = $("#Text6").val();
        var data = {};
        if (a == b) {
           data.CName = $("#Text1").val(),
           data.CPhone = $("#Text2").val(),
               data.CAccount = $("#Text4").val(),
                data.CPwd = a
            $.post('http://localhost:60539/Five/zhuce', data, function (d) {
                alert(d.mag);
                //跳转
            })
        } else {
            alert("两次密码输入不一致");
        }
        

    }
</script>
